{
    "plugins": [
        "react"
    ],
    "extends": [
        "standard", 
        "plugin:react/recommended"
    ],

    "rules": {
        "indent": ["error", 2],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-unused-vars": 0
    },

    "globals": { "fetch": false }
}